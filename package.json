		"figmagic": "node --experimental-modules ./node_modules/figmagic/index.mjs",
		"figmagic:graphics": "node --experimental-modules ./node_modules/figmagic/index.mjs --syncGraphics",
		"figmagic:elements": "node --experimental-modules ./node_modules/figmagic/index.mjs --syncElements",
		"figmagic:sync": "node --experimental-modules ./node_modules/figmagic/index.mjs --syncElements --syncGraphics",
		"storybook": "start-storybook"
